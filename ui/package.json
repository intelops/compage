{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.200",
    "axios": "^1.6.0",
    "dagre": "^0.8.5",
    "diagram-maker": "^2.0.1",
    "node-sass": "^9.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-json-pretty": "^2.2.0",
    "react-loading-overlay-ts": "^2.0.2",
    "react-redux": "^8.1.3",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "build:css": "node-sass src/ -o src/",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "dev-start": "PORT=3001 react-scripts start",
    "start": "HOST=0.0.0.0 PORT=3001 react-scripts start",
    "k8s-start": "PORT=3000 react-scripts start",
    "build": "react-app-env --env-file=.env.development build",
    "build:production": "react-app-env --env-file=.env.production build",
    "test": "react-app-env --env-file=.env.development test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^18.18.7",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-redux": "^7.1.28",
    "@types/react-redux-toastr": "^7.6.4",
    "assert": "^2.1.0",
    "axios": "^1.6.0",
    "dagre": "^0.8.5",
    "diagram-maker": "^2.0.1",
    "eslint-config-react-app": "^7.0.1",
    "lodash": "^4.17.21",
    "node-sass": "^9.0.0",
    "prop-types": "^15.8.1",
    "react-app-env": "^1.2.3",
    "react-beforeunload": "^2.6.0",
    "react-redux": "^8.1.3",
    "react-redux-toastr": "^7.6.12",
    "react-router-dom": "^6.17.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "reduxjs-toolkit-persist": "^7.2.1",
    "sass": "^1.69.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  }
}
